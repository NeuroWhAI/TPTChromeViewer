{
  "manifest_version": 2,

  "name": "TPT Chrome Viewer",
  "description": "Show TPT image from ID in clipboard.",
  "version": "1.0",

  "permissions": [
    "http://cafe.naver.com/powdertoy/*",
    "tabs",
    "clipboardRead"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
